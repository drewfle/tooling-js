{
  "name": "@drewfle/examples",
  "private": true,
  "description": "",
  "version": "1.1.19",
  "scripts": {
    "build:ts-node": "drewfle-bundler rollup --src src-ts-node/index.ts --dist dist-ts-node/bundle.js",
    "build:ts-react": "drewfle-bundler rollup --src src-ts-react/index.tsx --dist dist-ts-react/bundle.js -o browser",
    "watch:ts-react": "drewfle-bundler rollup --src src-ts-react/index.tsx --dist dist-ts-react/bundle.js -o browser --watch --serve",
    "build:ts-react:esbuild": "drewfle-bundler rollup --src src-ts-react/index.tsx --dist dist-ts-react/bundle.js -o browser -c esbuild",
    "watch:ts-react:esbuild": "drewfle-bundler rollup --src src-ts-react/index.tsx --dist dist-ts-react/bundle.js -o browser --watch --serve -c esbuild",
    "init:ts-react": "drewfle-bundler rollup --init ts-react",
    "tmp": "drewfle-bundler -h",
    "tmp2": "drewfle-bundler rollup -h",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@drewfle/bundler": "^1.4.8",
    "@drewfle/config": "^0.1.6",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.9.8",
    "typescript": "3.9.6"
  },
  "author": "Andrew Liu <andrewliustudio@gmail.com>",
  "license": "ISC"
}
